{
  "version": 2,
  "builds": [
    {
      "src": "api/callback.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/callback",
      "dest": "api/callback"  // 修改这里，去掉 .js 扩展名
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
